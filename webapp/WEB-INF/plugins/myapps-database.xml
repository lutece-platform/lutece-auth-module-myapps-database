<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?><plug-in>
    
    <!-- Plugin Informations --> 
    <name>myapps-database</name>
    <class>fr.paris.lutece.plugins.myapps.modules.database.service.MyAppsDatabasePlugin</class>
    <version>1.0.1</version>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <description>module.myapps.database.module.description</description>
    <provider>module.myapps.database.module.provider</provider>
    <provider-url>http://lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/plugins/myapps/myapps.png</icon-url>
    <copyright>Copyright 2001-2010 Mairie de Paris</copyright>
    <core-version-dependency>
		<min-core-version>2.4.3</min-core-version>
		<max-core-version/>	
	</core-version-dependency>
    <db-pool-required>1</db-pool-required>
    
    <!--Administration interface parameters-->
    <admin-features>
        <admin-feature>
            <feature-id>MYAPPS_DATABASE_MANAGEMENT</feature-id>
            <feature-title>module.myapps.database.adminFeature.myapps_database_management.name</feature-title>
            <feature-description>module.myapps.database.adminFeature.myapps_database_management.description</feature-description>
            <feature-level>1</feature-level>
            <feature-url>jsp/admin/plugins/myapps/modules/database/ManageMyApps.jsp</feature-url>
        </admin-feature>
    </admin-features>
    
    <!-- Applications -->
    <applications>
        <application>
            <application-id>myapps-database</application-id>
            <application-class>fr.paris.lutece.plugins.myapps.modules.database.web.MyAppsDatabaseApp</application-class>
        </application>
    </applications>
    
    <!-- RBAC Resources -->
	<rbac-resource-types>
		<rbac-resource-type>
			<rbac-resource-type-class>
				fr.paris.lutece.plugins.myapps.modules.database.service.MyAppsDatabaseResourceIdService
			</rbac-resource-type-class>
		</rbac-resource-type>
	</rbac-resource-types>
    
</plug-in>